    {
      "name": "Launch for cluster static",
      "type": "python",
      "cwd": "${workspaceRoot}/sge",
      "request": "launch",
      "program": "examples/symreg.py",
      "console": "integratedTerminal",
      "args": ["--experiment_name", "dumps/debug_static", 
      "--parameters", "parameters/standard_static.yml", 
       "--grammar", "grammars/regression_pagie.pybnf"
